        "columns": ["timestamp", "security.event_type", "security.severity"],
        "show_date_column": true,
        "show_message_column": true,
        "message_display": "expanded-md"
      },
      "layout": {
        "x": 0,
        "y": 15,
        "width": 6,
        "height": 3
      }
    },
    {
      "id": 14,
