{
  "title": "ObservAI Hub - LLM Observability Dashboard",
  "description": "Competition-grade observability for LLM applications with Vertex AI Gemini",
  "widgets": [
    {
      "id": 1,
      "definition": {
        "title": "ðŸŽ¯ LLM Request Overview",
        "title_size": "16",
        "title_align": "left",
        "type": "query_value",
        "requests": [
          {
            "formulas": [
              {
                "formula": "query1"
              }
            ],
            "queries": [
              {
                "data_source": "rum",
                "name": "query1",
                "search": {
                  "query": "@type:action @action.target.name:llm_inference"
                },
                "compute": {
                  "aggregation": "count"
                },
                "group_by": [],
                "indexes": ["*"]
              }
            ],
            "response_format": "scalar"
          }
        ],
        "autoscale": true,
        "precision": 0
      },
      "layout": {
        "x": 0,
        "y": 0,
        "width": 3,
        "height": 2
      }
    },
    {
      "id": 2,
      "definition": {
        "title": "âš¡ Average Latency (ms)",
        "title_size": "16",
        "title_align": "left",
        "type": "query_value",
        "requests": [
          {
            "formulas": [
              {
                "formula": "query1"
              }
            ],
            "queries": [
              {
                "data_source": "rum",
                "name": "query1",
                "search": {
                  "query": "@type:action @action.target.name:llm_inference"
                },
                "compute": {
                  "aggregation": "avg",
                  "metric": "@action.llm.latency_ms"
                },
                "group_by": [],
                "indexes": ["*"]
              }
