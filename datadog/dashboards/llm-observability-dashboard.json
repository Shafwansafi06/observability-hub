        "type": "log_stream",
        "query": "service:ml-observability",
        "columns": ["timestamp", "ml.model", "ml.quality.coherence", "ml.quality.hallucination_prob"],
        "show_date_column": true,
        "show_message_column": true,
        "message_display": "expanded-md"
