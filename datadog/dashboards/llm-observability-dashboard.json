      "layout": {
        "x": 3,
        "y": 0,
        "width": 3,
        "height": 2
      }
    },
    {
      "id": 3,
      "definition": {
        "title": "ðŸ§  Total Tokens Processed",
        "title_size": "16",
        "title_align": "left",
        "type": "query_value",
        "requests": [
          {
            "formulas": [
              {
                "formula": "query1"
              }
            ],
            "queries": [
              {
                "data_source": "rum",
                "name": "query1",
                "search": {
                  "query": "@type:action @action.target.name:llm_inference"
                },
                "compute": {
                  "aggregation": "sum",
                  "metric": "@action.llm.tokens.total"
                },
                "group_by": [],
