  "license": "MIT",
  "peerDependencies": {
    "@google/generative-ai": "^0.24.0"
  },
  "dependencies": {
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "tsup": "^8.0.1",
    "typescript": "^5.6.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Shafwansafi06/observability-hub"
  },
  "bugs": {
